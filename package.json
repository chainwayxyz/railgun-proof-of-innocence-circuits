    "postinstall": "patch-package"
  },
  "devDependencies": {
    "circom_tester": "0.0.19",
    "mocha": "^9.2.0",
    "patch-package": "^8.0.0"
  },
  "dependencies": {
    "circomlib": "2.0.5"
  }
}
